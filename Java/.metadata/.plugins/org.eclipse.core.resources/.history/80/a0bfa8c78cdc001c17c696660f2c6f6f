
public class TestaPontoFlutuante {
	
		public static void main(String[] args) {
			
			double salario;
			salario = 1250.70;
			System.out.println("Meu salario é " + salario);
			
			System.out.printf("Meu salário é %.2f", salario);
			
			double idade = 22;
			
			double divisao = 33.15/2;
			
			System.out.println(divisao);

		}
}
